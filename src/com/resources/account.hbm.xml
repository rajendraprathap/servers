<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">


<hibernate-mapping>
<class name="com.model.Account">
  <id name="username" type="string" length="15"/>
  <property name="password" type="string" length="15"/>
  <property name="fname" type="string" length="15"/>
  <property name="email" type="string" length="20"/>
  <property name="address" type="string" length="20"/>
  

</class>



</hibernate-mapping>